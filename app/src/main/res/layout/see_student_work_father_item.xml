<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="33dp"
    android:orientation="horizontal"
    android:background="@color/see_student_work_father_item_bg"
    >
    <TextView
        android:id="@+id/tvQuizName"
        android:layout_gravity="bottom"
        android:layout_marginBottom="3dp"
        android:layout_marginLeft="@dimen/margin_size13"
        android:text="查查"
        android:textSize="@dimen/text_size12"
        android:singleLine="true"
        android:ellipsize="end"
        android:textColor="@color/tv_color_see_work_quizname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <TextView
        android:id="@+id/tvCount"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:gravity="right"
        android:layout_weight="1"
        android:layout_marginBottom="3dp"
        android:layout_marginRight="@dimen/margin_size13"
        android:text="222sdfasdf"
        android:textSize="@dimen/text_size12"
        android:singleLine="true"
        android:textColor="@color/tv_color_see_work_quizname"
        />
</LinearLayout>